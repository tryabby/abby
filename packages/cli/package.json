{
  "name": "@tryabby/cli",
  "version": "0.0.0",
  "private": true,
  "main": "./lib/index.js",
  "bin": {
    "abby": "./lib/index.js"
  },
  "scripts": {
    "start": "pnpm run build && ./dist/index.js",
    "start2": "ts-node src/index.ts",
    "start:windows": "nodemon --watch 'src/**/*.ts' --exec \"npx ts-node\" src/index.ts",
    "create": "npm run build && npm run test",
    "build": "esbuild src/index.ts --bundle --target=node10.4 --bundle --platform=node --format=cjs --outfile=dist/index.js --external:figlet",
    "build:prod": "tsc --noEmit && pnpm run build",
    "buildv2": "npx tsc",
    "local": "sudo npm i -g && pizza",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "clear": "^0.1.0",
    "commander": "^10.0.1",
    "figlet": "^1.6.0",
    "node-fetch": "^3.3.1",
    "path": "^0.12.7",
    "stringify-object": "^5.0.0",
    "tsup": "^6.5.0"
  },
  "devDependencies": {
    "@tryabby/core": "workspace:^",
    "@types/figlet": "^1.5.6",
    "@types/node": "^20.3.1",
    "@types/stringify-object": "^4.0.2",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.3"
  }
}