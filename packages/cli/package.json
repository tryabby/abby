{
  "name": "@tryabby/cli",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/index.js",
  "bin": {
    "abby": "./dist/index.js"
  },
  "scripts": {
    "dev": "pnpm run build --watch",
    "start": "pnpm run build && ./dist/index.js",
    "create": "npm run build && npm run test",
    "build": "esbuild src/index.ts --bundle --target=node10.4 --bundle --platform=node --format=cjs --outfile=dist/index.js --external:figlet  --external:prettier",
    "build:prod": "tsc --noEmit && pnpm run build",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "test": "vitest"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "clear": "^0.1.0",
    "commander": "^10.0.1",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.0.3",
    "esbuild": "0.18.17",
    "figlet": "^1.6.0",
    "msw": "^1.2.2",
    "node-fetch": "^3.3.1",
    "prettier": "^3.0.0",
    "tsup": "^6.5.0",
    "unconfig": "^0.3.10",
    "vite": "^4.4.8",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tryabby/core": "workspace:^",
    "@types/figlet": "^1.5.6",
    "@types/node": "^20.3.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "typescript": "^5.1.3",
    "vitest": "^0.33.0"
  }
}