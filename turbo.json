{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "$VERCEL_URL",
    "$PORT",
    "$NODE_ENV",
    "$VERCEL",
    "$VERCEL_ENV",
    "$NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "build:prod": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    }
  }
}